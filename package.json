{
  "name": "ts-express-vue3",
  "version": "1.0.0",
  "description": "Boilerplate for Express + Vue 3 with Typescript & Composition API",
  "private": true,
  "author": "jason-napolitano",
  "scripts": {
    "serve": "concurrently \"npm run serve:server\" \"npm run serve:client\" --raw",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "dist": "concurrently \"npm run dist:server\" \"npm run dist:client\"",
    "dist:client": "serve -s ./dist/client",
    "dist:server": "serve -s ./dist/server",
    "serve:client": "vue-cli-service serve",
    "build:client": "vue-cli-service build",
    "serve:server": "nodemon --config ./nodemon.config.json",
    "build:server": "rimraf dist/server && tsc -p tsconfig.server.json",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "argon2": "^0.27.2",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.0.0-beta3",
    "bootstrap-icons": "^1.4.1",
    "cleave.js": "^1.6.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "jsbarcode2": "^3.9.3",
    "lodash": "^4.17.20",
    "peer": "^0.6.1",
    "register-service-worker": "^1.7.2",
    "socket.io": "^4.0.1",
    "sweetalert2": "^10.16.6",
    "uuid": "^8.3.2",
    "vue": "^3.0.0-0",
    "vue-class-component": "^8.0.0-0",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.3",
    "@vue/cli-plugin-eslint": "^4.5.3",
    "@vue/cli-plugin-router": "^4.5.3",
    "@vue/cli-plugin-typescript": "^4.5.3",
    "@vue/cli-plugin-vuex": "~4.5.3",
    "@vue/cli-service": "^4.5.3",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "concurrently": "^5.1.0",
    "eslint": "^7.7.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-0",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.2",
    "ts-node": "^8.10.2",
    "typescript": "~3.9.3"
  }
}
